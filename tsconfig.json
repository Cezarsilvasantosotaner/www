    "compilerOptions": {
        "esModuleInterop": true, // Only enabled from compatibility with "tagged-template-noop". Potentially can be refactored away.
        "outDir": ".tscache",
        "incremental": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "jsx": "react",
        "baseUrl": "./",
        "strict": true,
        "lib": ["dom", "es6", "esnext.asynciterable"]
    }
}
